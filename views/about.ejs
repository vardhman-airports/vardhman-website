  <style>
    .hero-section {
      padding: 0 20px;
      text-align: center;
      position: relative;
      overflow: hidden;
    }


    @keyframes float {

      0%,
      100% {
        transform: translateY(0px);
      }

      50% {
        transform: translateY(-10px);
      }
    }

    .hero-content {
      max-width: 1200px;
      margin: 0 10%;
      position: relative;
      z-index: 1;
    }

    .hero-content h1 {
      font-size: 3rem;
      font-weight: 600;
      margin-bottom: 20px;
      letter-spacing: -0.025em;
    }

    .hero-content p {
      font-size: 1.3rem;
      opacity: 0.95;
      font-weight: 600;
      margin: 0;
    }

    .timeline-container {
      max-width: 1100px;
      margin: 0 auto;
      padding: 80px 20px;
      position: relative;
    }

    .timeline {
      position: relative;
    }

    .timeline::before {
      content: '';
      position: absolute;
      left: 50%;
      top: 0;
      bottom: 0;
      width: 3px;
      background: linear-gradient(to bottom, #E2E8F0, #CBD5E0);
      transform: translateX(-50%);
      border-radius: 2px;
    }

    .timeline-item {
      margin-bottom: 60px;
      position: relative;
      opacity: 0;
      transform: translateY(20px);
      transition: all 0.6s ease;
    }

    .timeline-item.visible {
      opacity: 1;
      transform: translateY(0);
    }

    .timeline-item:nth-child(odd) {
      padding-right: 50%;
      padding-left: 0;
    }

    .timeline-item:nth-child(even) {
      padding-left: 50%;
      padding-right: 0;
    }

    .timeline-content {
      background: white;
      padding: 35px;
      border-radius: 16px;
      box-shadow: 0 2px 20px rgba(0, 0, 0, 0.06);
      position: relative;
      border: 1px solid #F7FAFC;
      transition: all 0.3s ease;
    }

    .timeline-content:hover {
      transform: translateY(-3px);
      box-shadow: 0 8px 30px rgba(74, 144, 226, 0.1);
    }

    .timeline-item:nth-child(odd) .timeline-content {
      margin-right: 35px;
    }

    .timeline-item:nth-child(even) .timeline-content {
      margin-left: 35px;
    }

    .timeline-content::before {
      content: '';
      position: absolute;
      top: 50%;
      width: 0;
      height: 0;
      border: 12px solid transparent;
      transform: translateY(-50%);
    }

    .timeline-item:nth-child(odd) .timeline-content::before {
      right: -24px;
      border-left: 12px solid white;
    }

    .timeline-item:nth-child(even) .timeline-content::before {
      left: -24px;
      border-right: 12px solid white;
    }

    .timeline-marker {
      position: absolute;
      left: 50%;
      top: 45%;
      transform: translateX(-50%);
      width: 50px;
      height: 50px;
      background: linear-gradient(135deg, #4A90E2 0%, #357ABD 100%);
      border-radius: 50%;
      display: flex;
      align-items: center;
      justify-content: center;
      font-weight: 600;
      color: white;
      font-size: 0.9rem;
      z-index: 10;
      box-shadow: 0 4px 15px rgba(74, 144, 226, 0.25);
      transition: all 0.3s ease;
      border: 3px solid white;
    }

    .timeline-marker:hover {
      transform: translateX(-50%) scale(1.05);
      box-shadow: 0 6px 20px rgba(74, 144, 226, 0.3);
    }

    .year-badge {
      display: inline-block;
      background: #F0F7FF;
      color: #4A90E2;
      padding: 6px 14px;
      border-radius: 20px;
      font-size: 0.85rem;
      font-weight: 600;
      margin-bottom: 12px;
      letter-spacing: 0.5px;
      border: 1px solid #E3F2FD;
    }

    .content-title {
      font-size: 1.5rem;
      font-weight: 600;
      color: #1A202C;
      margin-bottom: 12px;
      letter-spacing: -0.01em;
    }

    .content-description {
      color: #4A5568;
      font-size: 1rem;
      line-height: 1.6;
      margin-bottom: 18px;
    }

    .highlight {
      background: linear-gradient(120deg, #ffe0e0 0%, #e3e4fd 100%);
      padding: 2px 6px;
      border-radius: 4px;
      font-weight: 500;
      color: #2D3748;
      border: 1px solid #F0F7FF;
    }

    .achievement-tag {
      display: inline-block;
      background: #F8FAFC;
      color: #4A90E2;
      padding: 6px 14px;
      border-radius: 20px;
      font-size: 0.85rem;
      font-weight: 500;
      border: 1px solid #E2E8F0;
      transition: all 0.3s ease;
    }

    .achievement-tag:hover {
      background: #4A90E2 !important;
      color: white !important;
      border-color: #4A90E2 !important;
    }

    .stats-section {
      background: #F8FAFC;
      padding: 70px 20px;
      margin-top: 40px;
      border-top: 1px solid #E2E8F0;
    }

    .stats-container {
      max-width: 1000px;
      margin: 0 auto;
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(220px, 1fr));
      gap: 30px;
      text-align: center;
    }

    .stat-item {
      background: white;
      padding: 35px 25px;
      border-radius: 16px;
      box-shadow: 0 2px 15px rgba(0, 0, 0, 0.04);
      transition: all 0.3s ease;
      border: 1px solid #F1F5F9;
    }

    .stat-item:hover {
      transform: translateY(-2px);
      box-shadow: 0 8px 25px rgba(74, 144, 226, 0.08);
    }

    .stat-number {
      font-size: 2.5rem;
      font-weight: 700;
      color: #4A90E2;
      margin-bottom: 8px;
    }

    .stat-label {
      font-size: 1.2rem;
      color: #4A5568;
      font-weight: 500;
    }

    .scroll-indicator {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 3px;
      background: rgba(74, 144, 226, 0.1);
      z-index: 1000;
    }

    .scroll-progress {
